<script>
        @if(TempData[Notify.notifyMessage] != null){
            @: Notify.show(@(TempData[Notify.notifyStatus]??2), '@TempData[Notify.notifyMessage]');
            if((int)(TempData[Notify.notifyStatus] ?? Notify.success) == Notify.success) {
                @: toastr.success('@TempData[Notify.notifyMessage]');
            } else if((int)TempData[Notify.notifyStatus] == Notify.warning) {
                @: toastr.warning('@TempData[Notify.notifyMessage]');
            } else if((int)TempData[Notify.notifyStatus] == Notify.danger) {
                @: toastr.error('@TempData[Notify.notifyMessage]');
            }
        }
        Notify.show(1599963920087, "<b>VNTT</b> kính chào các bạn!");
</script>